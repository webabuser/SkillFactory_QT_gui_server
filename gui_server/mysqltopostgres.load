LOAD DATABASE
  FROM mysql://chat:chat@localhost/chat
  INTO postgresql://alex:1@localhost/chat_from_mysql
WITH 
  create tables,
  create indexes,
  reset sequences,
  foreign keys
SET
  maintenance_work_mem to '128MB',
  work_mem to '12MB'
CAST
  type datetime to timestamptz drop default drop not null using zero-dates-to-null,
  type date drop not null drop default using zero-dates-to-null;